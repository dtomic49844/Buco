@page
@model LogoutModel
@{
    ViewData["Title"] = "Log out";
}

<div class="wrapper text-center">
    <h1>@ViewData["Title"]</h1>
    <p>You have successfully logged out of the application!</p>
    <img class="img-fluid" src="~/images/cat-exercise-photos-1.jpg"/>
    <a asp-action="Index" asp-controller="Home">Home</a> | <a asp-area="Identity" asp-page="/Account/Register">Register</a> | <a asp-area="Identity" asp-page="/Account/Login">Login</a>
</div>